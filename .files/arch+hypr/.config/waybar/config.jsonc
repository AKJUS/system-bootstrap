{
  "layer": "top", // Waybar at top layer
  "position": "top", // Waybar position (top|bottom|left|right)
  "height": 14, // Waybar height (to be removed for auto height)
  // "width": 1280, // Waybar width
  // "gtk-layer-shell": "false",
  // Choose the order of the modules
  "modules-left": [
    "custom/icon",
    "hyprland/workspaces",
    "hyprland/windowcount",
    "hyprland/window",
    "hyprland/submap",
    "mpris",
  ],
  "modules-center": [
    // "custom/spotify",
    "clock",
  ],
  "modules-right": [
    "custom/privacycheck",
    "idle_inhibitor",
    "privacy",
    "hyprland/language",
    "tray",
    "custom/zypper",
    "network",
    "bluetooth",
    "cpu",
    "memory",
    "temperature",
    //"group/hardware",
    "wireplumber",
    "wireplumber#source",
    //"pulseaudio",
    "battery",
    "power-profiles-daemon",
    "custom/power",
  ],
  "group/hardware": {
    "orientation": "inherit",
    "drawer": {
      "transition-duration": 500,
      "children-class": "not-power",
      "transition-left-to-right": false,
    },
    "modules": [
      "cpu",
      "memory",
      "temperature"
    ],
  },
  "wireplumber#source": {
    "node-type": "Audio/Source",
    "format": "<span>Ó¥É</span> <span>{volume}%</span>",
    "format-muted": "<span>Û∞ç≠</span>",
    "on-click-right": "pavucontrol",
    "on-click": "wpctl set-mute @DEFAULT_AUDIO_SOURCE@ toggle",
    "scroll-step": 5,
    "tooltip": true,
    "tooltip-format": "{source_desc} {volume}%",
    "reverse-scrolling": true,
  },
  "wireplumber": {
    "format": "<span>{icon}</span> {volume}%",
    "format-muted": "<span>Û∞ñÅ </span>",
    "on-click-right": "pavucontrol",
    "on-click": "wpctl set-mute @DEFAULT_AUDIO_SINK@ toggle",
    "format-icons": [
      "ÔÄ¶",
      "ÔÄß",
      "ÔÄ®"
    ],
    "scroll-step": 5,
    "tooltip": true,
    "tooltip-format": "{node_name} {volume}%",
    "reverse-scrolling": true,
  },
  "custom/privacycheck": {
    "return-type": "json",
    "exec": "~/.config/waybar/scripts/privacy-check.sh",
    "format": "{}",
    "signal": 8,
    "interval": 10,
  },
  "custom/spotify": {
    "return-type": "json",
    "exec": "~/.config/waybar/scripts/spotify/spotify.sh artist",
    "format": "Û∞ìá <span>{}</span>",
    "interval": 10,
    "on-click": "playerctl -p spotify play-pause",
    "exec-if": "playerctl --list-all | grep -iq spotify",
    "on-scroll-up": "playerctl -p spotify next",
    "on-scroll-down": "playerctl -p spotify next",
  },
  "custom/icon": {
    "tooltip": true,
    "tooltip-format": "<span color='#c4bd99'><tt><small>CachyOS</small></tt></span>",
    "format": "Û∞£á",
    "on-click": "~/.config/rofi/launchers/misc/launcher.sh &"
  },
  "custom/separator-icon": {
    "format": " ÓÇª",
    "tooltip": false,
  },
  "hyprland/submap": {
    "format": "Ó¨ë {}",
    "max-length": 8,
    "tooltip": true
  },
  "hyprland/windowcount": {
    "format": "Óâ≠ {}",
    "format-empty": "Ôì¥ {}",
    "format-windowed": "Ó≠ø {}",
    "separate-outputs": true
  },
  "hyprland/window": {
    "format": "{title}",
    "rewrite": {
      "(.*) ‚Äî Mozilla Firefox": "$1",
    },
    "separate-outputs": true,
    "expand": true,
    "max-length": 25,
    "tooltip": true,
    "icon": true,
  },
  "hyprland/language": {
    "format": " Û∞åì {}",
    "format-en": "US Óº† ",
    "format-uk": "UA ÔÜ´ ",
  },
  //  Modules configuration
  "hyprland/workspaces": {
    "format-icons": {
      "1": "ÓùÖ",
      "2": "Óûµ",
      "3": "ÔÑ° ",
      "4": "ÔÜº ",
      "5": "Ó≠£ ",
      "6": "Ôá® ",
      "7": "Ôáø ",
      "8": "ÔÖº",
      "9": "Ó≠£",
      "active": "ÔÜí",
      "default": "ÔÑë",
      "urgent": "ÔÅ™",
      "occupied": "ÔÅ±",
      "empty": "ÔÅî",
    },
    "persistent-workspaces": {
      "*": 9, // 5 workspaces by default on every monitor
      "HDMI-A-1": 5 // but only three on HDMI-A-1
    },
    "format": "<sub>{icon}</sub>",
    "window-rewrite-default": "ÔÅô",
    "window-rewrite": {
      "title<.*youtube.*>": "ÔÖ™", // Windows whose titles contain "youtube"
      "class<firefox>": "Ôâ©", // Windows whose classes are "firefox"
      "class<firefox> title<.*github.*>": "ÔÑì", // Windows whose class is "firefox" and title contains "github". Note that "class" always comes first.
      "foot": "Óûï", // Windows that contain "foot" in either class or title. For optimization reasons, it will only match against a title if at least one other window explicitly matches against a title.
      "code": "Û∞®û",
    },
  },
  "idle_inhibitor": {
    "format": "{icon}",
    "format-icons": {
      "activated": "ÔÅÆ ",
      "deactivated": "ÔÅ∞ ",
    },
  },
  "tray": {
    "icon-size": 16,
    "spacing": 8,
  },
  "clock": {
    // "timezone": "America/New_York",
    "format": "ÔÄó {:%b %d %Y %R}",
    "format-alt": "{:%a %d %b w:%V %H:%M}",
    "tooltip-format": "<span color='#c4bd99'><tt><small>{calendar}</small></tt></span>",
    "calendar": {
      "mode-mon-col": 4,
      "weeks-pos": "left",
      "on-scroll": 1,
      "on-click-right": "mode",
      "format": {
        "months": "<span color='#c4bd99'><b>{}</b></span>",
        "weeks": "<span color='#935e7d'><b>{}</b></span>",
        "weekdays": "<span color='#e5ed78'><b>{}</b></span>",
        "today": "<span color='#e5ed78'><b><u>{}</u></b></span>",
      },
    },
    "actions": {
      "on-click-right": "mode",
      "on-scroll-up": "shift_up",
      "on-scroll-down": "shift_down",
    },
    "interval": 10,
  },
  "privacy": {
    "icon-spacing": 4,
    "icon-size": 18,
    "transition-duration": 250,
    "modules": [
      {
        "type": "screenshare",
        "tooltip": true,
        "tooltip-icon-size": 24,
      },
      {
        "type": "audio-out",
        "tooltip": true,
        "tooltip-icon-size": 24,
      },
      {
        "type": "audio-in",
        "tooltip": true,
        "tooltip-icon-size": 24,
      },
    ],
    "ignore-monitor": true,
    "ignore": [
      {
        "type": "audio-in",
        "name": "cava",
      },
      {
        "type": "screenshare",
        "name": "obs",
      },
    ],
  },
  "mpris": {
    "format": "<span>{player_icon}</span> {title}",
    "format-paused": "<span>{status_icon}</span> <span>{player_icon}</span> <i>{title}</i>",
    "player-icons": {
      "default": "‚ñ∂",
      "mpv": "üéµ",
      "spotify": "Û∞ìá",
      "vlc": "üéû",
      "firefox": "Ôâ©",
      "chromium": "ÓùÉ",
      "brave": "ÓùÉ",
      "opera": "ÓùÜ",
      "vivaldi": "ÓùÜ",
      "slack": "Ó¢§",
    },
    "status-icons": {
      "paused": "Ôäã",
    },
    "max-length": 25,
  },
  "cpu": {
    "format": "Û∞ª† {usage}% ",
    "tooltip": false,
    "on-click-right": "exec alacritty -e sh -c 'btm'",
  },
  "memory": {
    "format": "ÓøÖ {}%",
  },
  "temperature": {
    "critical-threshold": 80,
    "format": "{icon} {temperatureC}¬∞C",
    "format-icons": [
      "Óº´",
      "Ôãâ",
      "Óº™"
    ],
  },
  "network": {
    // "interface": "wlp2*", // (Optional) To force the use of this interface
    "format-wifi": "Ôá´",
    "format-ethernet": "Óºâ ",
    "format-linked": "Ó´É",
    "format-disconnected": "‚ö†",
    "format-alt": "Óºâ {ipaddr}",
    "tooltip-format": "{ifname} via {ipaddr} Ôäπ",
    "tooltip-format-wifi": "{essid} ({signalStrength}%) Ôá´ {ipaddr}",
    "tooltip-format-ethernet": "{ifname} ÔÉÅ  {ipaddr}",
    "tooltip-format-disconnected": "Disconnected",
  },
  "bluetooth": {
    "format": "Ôäî",
    "format-disabled": "Ôäî",
    "format-off": "Ôäî",
    "format-connected": "Ôäî {num_connections}",
    "tooltip-format": "{controller_address} {status}\n\n{num_connections} connected",
    "tooltip-format-disabled": "{status}",
    "tooltip-format-connected": "{controller_address}\n\n{num_connections} connected\n\n{device_enumerate}",
    "tooltip-format-enumerate-connected": "{device_alias}\t{device_address}",
    "format-connected-battery": "Ôäî {device_alias} {device_battery_percentage}%",
    "tooltip-format-enumerate-connected-battery": "{device_alias}\t{device_address}\t{device_battery_percentage}%",
    "on-click": "bluetooth toggle; pkill -SIGRTMIN+8 waybar",
    "on-click-right": "exec alacritty -e sh -c 'bluetoothctl'",
  },
  // Not used now because of the new wireplumber module
  "pulseaudio": {
    "format": "<span size='13000' foreground='#EBDDAA'>{icon}</span> {volume}%",
    "format-muted": "<span size='14000' foreground='#EBDDAA'>ÔÄ¶</span> Muted",
    "format-icons": {
      "headphone": "ÔÄ•",
      "hands-free": "Ôäî",
      "headset": "ÔÄ•",
      "phone": "ÔÇï",
      "portable": "ÔÇï",
      "car": "ÔÜπ",
      "default": [
        "ÔÄ¶",
        "ÔÄß",
        "ÔÄ®"
      ],
    },
    "format-alt": "{volume} {icon}",
    "format-alt-click": "click-middle",
    "scroll-step": 10,
    "on-click": "pactl set-sink-mute @DEFAULT_SINK@ toggle",
    "on-click-right": "pavucontrol",
    "tooltip": true,
  },
  "custom/notifications": {
    "tooltip": true,
    "format": "{icon}",
  },
  "battery": {
    "interval": 60,
    "states": {
      "warning": 30,
      "critical": 15
    },
    "format": "{capacity}% {icon}",
    "format-icons": [
      "ÔâÑ",
      "ÔâÉ",
      "ÔâÇ",
      "ÔâÅ",
      "ÔâÄ"
    ],
    "max-length": 25
  },
  "power-profiles-daemon": {
    "format": "{icon}",
    "tooltip-format": "Power profile: {profile}\nDriver: {driver}",
    "tooltip": true,
    "format-icons": {
      "default": "ÔÉß",
      "performance": "ÔÉß",
      "balanced": "Ôâé",
      "power-saver": "ÔÅ¨"
    }
  },
  "custom/power": {
    "format": "Û∞§Ü",
    "tooltip": false,
    "on-click": "~/.config/rofi/powermenu/type-2/powermenu.sh &"
  }
}
