output HDMI-A-1 {
    mode 3840x2160
    position 0,0 
    alias $ELGATO
}
output HDMI-A-2 {
    mode 2560x1440@119.998
    position 3840,0
    transform 90  
    adaptive_sync on
    alias $DELL
}

profile dual {
    output $ELGATO enable
    output $DELL enable
}
profile secondary {
    output $DELL disable
    output $ELGATO enable
}

profile main {
    output $ELGATO disable
    output $DELL enable
}

profile main-only {
    output $DELL enable
}
