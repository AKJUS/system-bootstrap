{
  "layer": "top", // Waybar at top layer
  "position": "top", // Waybar position (top|bottom|left|right)
  "height": 14, // Waybar height (to be removed for auto height)
  // "width": 1280, // Waybar width
  // "gtk-layer-shell": "false",
  // Choose the order of the modules
  "modules-left": [
    "custom/icon",
    "sway/workspaces",
    "custom/scratchpad",
    "sway/mode",
    "sway/window",
    "mpris",
  ],
  "modules-center": [
    // "custom/spotify",
    "clock",
  ],
  "modules-right": [
    "custom/privacycheck",
    "idle_inhibitor",
    "privacy",
    "sway/language",
    "tray",
    "custom/zypper",
    "network",
    "bluetooth",
    "cpu",
    "memory",
    "temperature",
    //"group/hardware",
    "wireplumber",
    "wireplumber#source",
    //"pulseaudio",
    "custom/notifications",
  ],
  "wireplumber#source": {
    "node-type": "Audio/Source",
    "format": "<span>Ó¥É</span> <span>{volume}%</span>",
    "format-muted": "<span>Û∞ç≠</span>",
    "on-click-right": "pavucontrol",
    "on-click": "wpctl set-mute @DEFAULT_AUDIO_SOURCE@ toggle",
    "scroll-step": 5,
    "tooltip": true,
    "tooltip-format": "{source_desc} {volume}%",
    "reverse-scrolling": true,
  },
  "wireplumber": {
    "format": "<span>{icon}</span> {volume}%",
    "format-muted": "<span>Û∞ñÅ </span>",
    "on-click-right": "pavucontrol",
    "on-click": "wpctl set-mute @DEFAULT_AUDIO_SINK@ toggle",
    "format-icons": [
      "ÔÄ¶",
      "ÔÄß",
      "ÔÄ®"
    ],
    "scroll-step": 5,
    "tooltip": true,
    "tooltip-format": "{node_name} {volume}%",
    "reverse-scrolling": true,
  },
  "custom/privacycheck": {
    "return-type": "json",
    "exec": "~/.config/waybar/scripts/privacy-check.sh",
    "format": "{}",
    "signal": 8,
    "interval": 10,
  },
  "custom/spotify": {
    "return-type": "json",
    "exec": "~/.config/waybar/scripts/spotify/spotify.sh artist",
    "format": "Û∞ìá <span>{}</span>",
    "interval": 10,
    "on-click": "playerctl -p spotify play-pause",
    "exec-if": "playerctl --list-all | grep -iq spotify",
    "on-scroll-up": "playerctl -p spotify next",
    "on-scroll-down": "playerctl -p spotify next",
  },
  "custom/icon": {
    "tooltip": true,
    "tooltip-format": "<span color='#c4bd99'><tt><small>OpenSUSE Tumbleweed</small></tt></span>",
    "format": "ÓΩ≠ ",
  },
  "custom/separator-icon": {
    "format": " ÓÇª",
    "tooltip": false,
  },
  "sway/mode": {
    "format": "ÔÅîÔÑÖ {}",
  },
  "sway/window": {
    "format": "{title}",
    "expand": true,
    "max-length": 55,
    "tooltip": true,
    "icon": true,
  },
  "sway/language": {
    "on-click": "swaymsg input type:keyboard xkb_switch_layout next",
    "format": " Û∞åì {short} {variant}",
  },
  //  Modules configuration
  "sway/workspaces": {
    "disable-scroll": true,
    "sort-by-number": true,
    "all-outputs": false,
    "format": "{icon}",
    "format-icons": {
      "1": "ÓùÖ",
      "2": "Óûµ",
      "3": "ÔÑ° ",
      "4": "ÔÜº ",
      "5": "Ó≠£ ",
      "6": "Ôá® ",
      "7": "Ôáø ",
      "8": "ÔÖº",
      "9": "Ó≠£",
    },
  },
  "idle_inhibitor": {
    "format": "{icon}",
    "format-icons": {
      "activated": "ÔÅÆ ",
      "deactivated": "ÔÅ∞ ",
    },
  },
  "tray": {
    "icon-size": 16,
    "spacing": 8,
  },
  "clock": {
    // "timezone": "America/New_York",
    "format": "ÔÄó {:%b %d %Y %R}",
    "format-alt": "{:%a %d %b w:%V %H:%M}",
    "tooltip-format": "<span color='#c4bd99'><tt><small>{calendar}</small></tt></span>",
    "calendar": {
      "mode-mon-col": 4,
      "weeks-pos": "left",
      "on-scroll": 1,
      "on-click-right": "mode",
      "format": {
        "months": "<span color='#c4bd99'><b>{}</b></span>",
        "weeks": "<span color='#935e7d'><b>{}</b></span>",
        "weekdays": "<span color='#e5ed78'><b>{}</b></span>",
        "today": "<span color='#e5ed78'><b><u>{}</u></b></span>",
      },
    },
    "actions": {
      "on-click-right": "mode",
      "on-scroll-up": "shift_up",
      "on-scroll-down": "shift_down",
    },
    "interval": 10,
  },
  "privacy": {
    "icon-spacing": 4,
    "icon-size": 18,
    "transition-duration": 250,
    "modules": [
      {
        "type": "screenshare",
        "tooltip": true,
        "tooltip-icon-size": 24,
      },
      {
        "type": "audio-out",
        "tooltip": true,
        "tooltip-icon-size": 24,
      },
      {
        "type": "audio-in",
        "tooltip": true,
        "tooltip-icon-size": 24,
      },
    ],
    "ignore-monitor": true,
    "ignore": [
      {
        "type": "audio-in",
        "name": "cava",
      },
      {
        "type": "screenshare",
        "name": "obs",
      },
    ],
  },
  "mpris": {
    "format": "<span>{player_icon}</span> {title}",
    "format-paused": "<span>{status_icon}</span> <span>{player_icon}</span> <b>{title}</b>",
    "player-icons": {
      "default": "‚ñ∂",
      "mpv": "üéµ",
      "spotify": "Û∞ìá",
      "vlc": "üéû",
      "firefox": "Ôâ©",
      "chromium": "ÓùÉ",
      "brave": "ÓùÉ",
      "opera": "ÓùÜ",
      "vivaldi": "ÓùÜ",
      "slack": "Ó¢§",
    },
    "status-icons": {
      "paused": "‚è∏",
    },
    "max-length": 25,
  },
  "cpu": {
    "format": "Û∞ª† {usage}% ",
    "tooltip": false,
    "on-click-right": "exec alacritty -e sh -c 'btm'",
  },
  "memory": {
    "format": "ÓøÖ {}%",
  },
  "temperature": {
    "critical-threshold": 80,
    "format": "{icon} {temperatureC}¬∞C",
    "format-icons": [
      "Óº´",
      "Ôãâ",
      "Óº™"
    ],
  },
  "network": {
    // "interface": "wlp2*", // (Optional) To force the use of this interface
    "format-wifi": "Ôá´",
    "format-ethernet": "Óºâ ",
    "format-linked": "Ó´É",
    "format-disconnected": "‚ö†",
    "format-alt": "Óºâ {ipaddr}",
    "tooltip-format": "{ifname} via {ipaddr} Ôäπ",
    "tooltip-format-wifi": "{essid} ({signalStrength}%) Ôá´",
    "tooltip-format-ethernet": "{ifname} ÔÉÅ",
    "tooltip-format-disconnected": "Disconnected",
  },
  "bluetooth": {
    "format": "Ôäî",
    "format-disabled": "Ôäî",
    "format-off": "Ôäî",
    "format-connected": "Ôäî {num_connections}",
    "tooltip-format": "{controller_address} {status}\n\n{num_connections} connected",
    "tooltip-format-disabled": "{status}",
    "tooltip-format-connected": "{controller_address}\n\n{num_connections} connected\n\n{device_enumerate}",
    "tooltip-format-enumerate-connected": "{device_alias}\t{device_address}",
    "format-connected-battery": "Ôäî {device_alias} {device_battery_percentage}%",
    "tooltip-format-enumerate-connected-battery": "{device_alias}\t{device_address}\t{device_battery_percentage}%",
    "on-click": "bluetooth toggle; pkill -SIGRTMIN+8 waybar",
    "on-click-right": "exec alacritty -e sh -c 'bluetoothctl'",
  },
  // Not used now because of the new wireplumber module
  "pulseaudio": {
    "format": "<span size='13000' foreground='#EBDDAA'>{icon}</span> {volume}%",
    "format-muted": "<span size='14000' foreground='#EBDDAA'>ÔÄ¶</span> Muted",
    "format-icons": {
      "headphone": "ÔÄ•",
      "hands-free": "Ôäî",
      "headset": "ÔÄ•",
      "phone": "ÔÇï",
      "portable": "ÔÇï",
      "car": "ÔÜπ",
      "default": [
        "ÔÄ¶",
        "ÔÄß",
        "ÔÄ®"
      ],
    },
    "format-alt": "{volume} {icon}",
    "format-alt-click": "click-middle",
    "scroll-step": 10,
    "on-click": "pactl set-sink-mute @DEFAULT_SINK@ toggle",
    "on-click-right": "pavucontrol",
    "tooltip": true,
  },
  "custom/scratchpad": {
    "format-text": "{}",
    "return-type": "json",
    "interval": 3,
    "exec": "/usr/share/openSUSEway/helpers/scratchpad-indicator.sh 2> /dev/null",
    "exec-if": "exit 0",
    "on-click": "swaymsg 'scratchpad show'",
    "on-click-right": "swaymsg 'move scratchpad'",
  },
  "custom/notifications": {
    "tooltip": true,
    "format": "{icon}",
    "format-icons": {
      "notification": "<span foreground='#eba0ac' weight='bold'>ÔÇ¢</span><span foreground='red'><small><sup>‚¨§</sup></small></span>",
      "none": "<span foreground='#eba0ac' weight='bold'>ÔÇ¢ </span>",
      "dnd-notification": "<span foreground='#7f849c' weight='bold'>Ôá∑</span><span foreground='red'><small><sup>‚¨§</sup></small></span>",
      "dnd-none": "<span foreground='#7f849c' weight='bold'>Ôá∑ </span>",
    },
    "return-type": "json",
    "exec-if": "which swaync-client",
    "exec": "swaync-client -swb",
    "on-click": "swaync-client -t -sw",
    "on-click-right": "swaync-client -d -sw",
    "escape": true,
  },
}
